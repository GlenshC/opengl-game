{
    "configurations": [
        {
            "name": "Win32",
            "defines": [
                "CIMGUI_USE_OPENGL3",
                "CIMGUI_USE_GLFW",
                "IMGUI_IMPL_API=extern \"C\"",
                "GC_DEBUG",
                "GS_TEXTURE_DEFAULT_PARAMETERS"
            ],
            "windowsSdkVersion": "10.0.22621.0",
            "compilerPath": "C:/msys64/ucrt64/bin/gcc.exe",
            "cStandard": "c17",
            "cppStandard": "c++17",
            "intelliSenseMode": "windows-gcc-x64",
            "compilerArgs": [
                "-Wall ",
                "-Wextra",
                "-Lvendor/GLFW",
                "-lglfw3"
            ],
            "includePath": [
                "${workspaceFolder}/src",
                "${workspaceFolder}/vendor",
                "${workspaceFolder}/include",
                "${workspaceFolder}/vendor/cimgui",
                "${workspaceFolder}/vendor/cimgui/imgui",
                "${workspaceFolder}/vendor/cimgui/imgui/backends"
            ]
        }
    ],
    "version": 4
}